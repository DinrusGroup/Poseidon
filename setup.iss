var
  AssociationsPage : TInputQueryWizardPage;
  DCheckBox: TCheckBox;
  DPrjCheckBox: TCheckBox;
  ShouldAssociateDFiles : Boolean;
    ShouldAssociateDPrjFiles : Boolean;
  
procedure InitializeWizard;
begin
  { Create the pages }

  AssociationsPage := CreateInputQueryPage(wpWelcome,
    'File Associations', 'Would you like to associate .d files to Elephant ?',
    'Please choose your file associations , then click Next.');

  DCheckBox := TCheckBox.Create(AssociationsPage);
  DCheckBox.Top := ScaleX(80);
  DCheckBox.Width := AssociationsPage.SurfaceWidth;
  DCheckBox.Height := ScaleY(25);
  DCheckBox.Caption := 'Associate D Files ( *.d) with Elephant';
  DCheckBox.Checked := True;
  DCheckBox.Parent := AssociationsPage.Surface;
  
  
  
  DPrjCheckBox := TCheckBox.Create(AssociationsPage);
  DPrjCheckBox.Top := ScaleX(120);
  DPrjCheckBox.Width := AssociationsPage.SurfaceWidth;
  DPrjCheckBox.Height := ScaleY(25);
  DPrjCheckBox.Caption := 'Associate Elephant Project Files ( *.dprj ) with Elephant';
  DPrjCheckBox.Checked := True;
  DPrjCheckBox.Parent := AssociationsPage.Surface;
//  AssociationsPage.Add('Company:', False);

end;

function NextButtonClick(CurPageID: Integer): Boolean;
begin
  { Validate certain pages before allowing the user to proceed }
  if CurPageID = AssociationsPage.ID then begin
    if DCheckBox.Checked = True then begin
      ShouldAssociateDFiles := True;
    end else begin
      ShouldAssociateDFiles := False;
    end;


    if DPrjCheckBox.Checked = True then begin
      ShouldAssociateDPrjFiles := True;
    end else begin
      ShouldAssociateDPrjFiles := False;
    end;



  end;
    Result := True;
end;

function CheckAssociateFiles() : Boolean ;
begin
  Result := ShouldAssociateDFiles;
end;

function CheckAssociateProjects() : Boolean ;
begin
  Result := ShouldAssociateDPrjFiles;
end;
