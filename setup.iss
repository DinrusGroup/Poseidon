; Script generated by the Inno Setup Script Wizard.
; SEE THE DOCUMENTATION FOR DETAILS ON CREATING INNO SETUP SCRIPT FILES!

[Setup]
AppName=Elephant IDE
AppVerName=Elephant 0.34
AppPublisher=34_elephants productions
DefaultDirName={pf}\Elephant
DefaultGroupName=Elephant
OutputBaseFilename=setup
Compression=lzma
SolidCompression=yes
WizardImageFile=si.bmp
WizardSmallImageFile=si_small.bmp

[Tasks]
Name: "desktopicon"; Description: "{cm:CreateDesktopIcon}"; GroupDescription: "{cm:AdditionalIcons}"; Flags: unchecked
Name: "quicklaunchicon"; Description: "{cm:CreateQuickLaunchIcon}"; GroupDescription: "{cm:AdditionalIcons}"; Flags: unchecked

[Files]
Source: "C:\Projects\Elephant\Release\Elephant.exe"; DestDir: "{app}"; Flags: ignoreversion
Source: "C:\Projects\Elephant\PCRE\pcre.dll"; DestDir: "{app}"; Flags: ignoreversion
Source: "C:\Projects\Elephant\SetupScript\XT3100Lib.dll"; DestDir: "{app}"; Flags: ignoreversion
Source: "C:\Projects\Elephant\SetupScript\SciLexer.dll"; DestDir: "{app}"; Flags: ignoreversion
Source: "C:\Projects\Elephant\SetupScript\pspec.ini"; DestDir: "{app}"; Flags: onlyifdoesntexist
Source: "C:\Projects\Elephant\SetupScript\eide.ini"; DestDir: "{app}";
; Source: "C:\Projects\Elephant\SetupScript\eide.ini"; DestDir: "{app}"; Flags: onlyifdoesntexist
Source: "C:\Projects\Elephant\SetupScript\eide.ini.abbs"; DestDir: "{app}"; Flags: onlyifdoesntexist
Source: "C:\Projects\Elephant\SetupScript\ProjectFiles\*.*"; DestDir: "{app}\ProjectFiles"; Flags: ignoreversion
Source: "C:\Projects\Elephant\SetupScript\Scripts\*.*"; DestDir: "{app}\Scripts"; Flags: ignoreversion
Source: "C:\Projects\Elephant\SetupScript\temp_object\*.*"; DestDir: "{app}\ProjectFiles"; Flags: ignoreversion
Source: "C:\Projects\Elephant\SetupScript\Tools\*.*"; DestDir: "{app}\Tools"; Flags: ignoreversion

; NOTE: Don't use "Flags: ignoreversion" on any shared system files

[Icons]
Name: "{group}\Elephant"; Filename: "{app}\Elephant.exe" ;
Name: "{group}\{cm:UninstallProgram,Elephant}"; Filename: "{uninstallexe}"
Name: "{userdesktop}\Elephant"; Filename: "{app}\Elephant.exe"; Tasks: desktopicon;
Name: "{userappdata}\Microsoft\Internet Explorer\Quick Launch\Elephant"; Filename: "{app}\Elephant.exe"; Tasks: quicklaunchicon;


[Run]
Filename: "{app}\Elephant.exe"; Description: "{cm:LaunchProgram,Elephant}"; Flags: nowait postinstall skipifsilent


[Registry]
Root: HKCU; Subkey: "Software\thirty_four_elephants\"; Flags: uninsdeletekey
Root: HKCU; Subkey: "Software\White Elephant\"; Flags: uninsdeletekey
Root: HKCU; Subkey: "Software\thirty_four_elephants\Elephant"; ValueType: string; ValueName: "InstallPath"; ValueData: "{app}"; Flags: uninsdeletekey


